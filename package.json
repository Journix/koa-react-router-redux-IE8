{
    "name": "ie8demo",
    "private": true,
    "version": "0.1.0",
    "dependencies": {
      "debug": "^2.6.8",
      "dot": "^1.1.1",
      "koa": "^2.0.0",
      "koa-body": "^2.1.0",
      "koa-bodyparser": "^3.0.0",
      "koa-convert": "^1.2.0",
      "koa-logger": "^2.0.0",
      "koa-mount": "^2.0.0",
      "koa-router": "^7.2.1",
      "koa-session": "^3.3.1",
      "koa-static": "^3.0.0",
      "koa-views": "^5.0.1",
      "query-string": "^5.0.0"
    },
    "devDependencies": {
      "babel-preset-es2015": "^6.13.2",
      "babel-preset-stage-2": "^6.13.0",
      "babel-register": "^6.9.0",
      "babel-cli": "^6.11.4",
      "babel-eslint": "^6.1.2",
      "concurrently": "^3.5.0",
      "eslint": "^3.3.1",
      "eslint-plugin-import": "^1.13.0",
      "eslint-config-airbnb-base": "^5.0.2",
      "nodemon": "^1.11.0"
    },
    "scripts": {
      "start": "babel-node ./bin/run",
      "debug": "node --inspect --debug ./bin/run",
      "build": "babel src -d prod --source-maps",
      "build:static": "cd static && npm run dist",
      "watch:addstatic": "concurrently \"npm run build:static\" \"npm run debug\"",
      "watch": "nodemon --watch src --exec npm run debug",
      "watch:all": "nodemon --watch src --watch static/src --exec npm run watch:addstatic",
      "watch:debug": "nodemon --ignore static/ --ignore test/ --exec npm run debug",
      "watch:prod": "nodemon --exec npm run start --production"
    }
  }
